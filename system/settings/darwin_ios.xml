<?xml version="1.0" encoding="utf-8" ?>
<settings>
  <section id="appearance">
    <category id="lookandfeel" label="166" help="36102">
      <group id="1">
        <setting id="lookandfeel.skin" type="addon" label="166" help="36103">
          <level>0</level>
          <default>skin.ariana.touch</default>
          <constraints>
            <addontype>xbmc.gui.skin</addontype>
          </constraints>
          <control type="button" format="addon">
            <show more="true" details="true">installed</show>
          </control>
        </setting>
        <setting id="lookandfeel.interfaceappearance" type="integer" label="22025" help="22029">
          <level>1</level>
          <default>2</default>
          <constraints>
            <options>interfaceappearance</options>
          </constraints>
          <dependencies>
            <dependency type="visible">
              <or>
                <condition setting="lookandfeel.skin" operator="is">skin.ariana</condition>
                <condition setting="lookandfeel.skin" operator="is">skin.ariana.touch</condition>
              </or>
            </dependency>
          </dependencies>
          <control type="list" format="string" />
        </setting>
      </group>
    </category>
  </section>
  <section id="videos">
    <category id="videoplayer">
      <group id="3">
        <setting id="videoplayer.adjustrefreshrate">
          <visible>false</visible>
        </setting>
        <setting id="videoplayer.pauseafterrefreshchange">
          <visible>false</visible>
        </setting>
      </group>
    </category>
    <category id="videoacceleration">
      <group id="1">
        <setting id="videoplayer.hqscalers">
          <visible>false</visible>
        </setting>
      </group>
    </category>
  </section>
  <section id="music">
    <category id="audiocds">
      <visible>false</visible>
    </category>
  </section>
  <section id="system">
    <category id="videoscreen">
      <group id="1">
        <setting id="videoscreen.fakefullscreen">
          <visible>false</visible>
        </setting>
        <setting id="videoscreen.blankdisplays">
          <visible>false</visible>
        </setting>
        <setting id="videoscreen.vsync" type="integer" label="13105" help="36356">
          <default>2</default> <!-- VSYNC_ALWAYS -->
          <visible>false</visible>
          <control type="list" format="string" />
        </setting>
      </group>
    </category>
    <category id="audiooutput">
      <group id="1">
        <setting id="audiooutput.audiodevice" type="string">
          <default>Default</default>
          <visible>false</visible>
        </setting>
        <setting id="audiooutput.supportdtshdcpudecoding" type="boolean">
          <default>false</default>
          <visible>false</visible>
        </setting>
      </group>
      <group id="3">
        <setting id="audiooutput.passthroughdevice" type="string">
          <default>Default</default>
          <visible>false</visible>
        </setting>
      </group>
    </category>
    <category id="input">
      <group id="1">
        <setting id="input.peripherals">
          <visible>false</visible>
        </setting>
        <setting id="input.appleremotemode">
          <visible>false</visible>
        </setting>
        <setting id="input.appleremotesequencetime">
          <visible>false</visible>
        </setting>
      </group>
      <group id="2">
        <setting id="input.enablemouse">
          <visible>false</visible>
        </setting>
      </group>
    </category>
    <category id="powermanagement">
      <group id="1">
        <visible>false</visible>
      </group>
    </category>
    <category id="mysql">
      <group id="1">
        <setting id="mysql.timeout" type="integer" label="36609" help="36632">
          <default>5</default>
          <control type="edit" format="integer" />
        </setting>                 
      </group>
    </category>
  </section>
</settings>
